#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/media/shilei/新加卷/VsCode/demoCode/userItem/sky-admin/sky-vitepress/node_modules/.pnpm/vitepress@1.1.3_@algolia+client-search@4.23.3_search-insights@2.13.0/node_modules/vitepress/bin/node_modules:/media/shilei/新加卷/VsCode/demoCode/userItem/sky-admin/sky-vitepress/node_modules/.pnpm/vitepress@1.1.3_@algolia+client-search@4.23.3_search-insights@2.13.0/node_modules/vitepress/node_modules:/media/shilei/新加卷/VsCode/demoCode/userItem/sky-admin/sky-vitepress/node_modules/.pnpm/vitepress@1.1.3_@algolia+client-search@4.23.3_search-insights@2.13.0/node_modules:/media/shilei/新加卷/VsCode/demoCode/userItem/sky-admin/sky-vitepress/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/media/shilei/新加卷/VsCode/demoCode/userItem/sky-admin/sky-vitepress/node_modules/.pnpm/vitepress@1.1.3_@algolia+client-search@4.23.3_search-insights@2.13.0/node_modules/vitepress/bin/node_modules:/media/shilei/新加卷/VsCode/demoCode/userItem/sky-admin/sky-vitepress/node_modules/.pnpm/vitepress@1.1.3_@algolia+client-search@4.23.3_search-insights@2.13.0/node_modules/vitepress/node_modules:/media/shilei/新加卷/VsCode/demoCode/userItem/sky-admin/sky-vitepress/node_modules/.pnpm/vitepress@1.1.3_@algolia+client-search@4.23.3_search-insights@2.13.0/node_modules:/media/shilei/新加卷/VsCode/demoCode/userItem/sky-admin/sky-vitepress/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/vitepress.js" "$@"
else
  exec node  "$basedir/../../bin/vitepress.js" "$@"
fi
